package entities;

import java.time.LocalDateTime;

import entities.enums.OrderStatus;

public class Order {
	private Integer id;
	private LocalDateTime moment;
	private OrderStatus status;

	public Order() {
	}

	public Order(Integer id, LocalDateTime moment, OrderStatus status) {
		super();
		this.id = id;
		this.moment = moment;
		this.status = status;
	}
	


}
