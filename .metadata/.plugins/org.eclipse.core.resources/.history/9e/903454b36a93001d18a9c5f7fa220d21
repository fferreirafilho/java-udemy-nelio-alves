package entities;

public class Account {
	private int number;
	private String holder;
	private double balance;
	

	public Account(int number, String holder, double balance) {
		super();
		this.number = number;
		this.holder = holder;
		this.balance = balance;
	}

	public Account(double balance) {
		super();
		this.balance = balance;
	}
}
